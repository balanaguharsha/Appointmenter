<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.researcher.appointmenter">

    <application
        android:name=".ReadyAndroidApp"
        android:allowBackup="true"
        android:icon="@drawable/amudalogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EntryActivity"
            android:noHistory="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BookAppointment"
            android:noHistory="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BookedAppointments"
            android:noHistory="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Splash"
            android:screenOrientation="portrait"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditProfile" android:noHistory="true" android:screenOrientation="portrait" />
        <activity android:name=".request_feature" android:noHistory="true" android:screenOrientation="portrait"></activity>
    </application>

</manifest>